{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "mytrader.tech,www.mytrader.tech,213.238.180.201",
  "ConnectionStrings": {
    "DefaultConnection": "Host=postgres;Port=5432;Database=mytrader;Username=postgres;Password=${POSTGRES_PASSWORD}"
  },
  "Jwt": {
    "Key": "${JWT_SECRET_KEY}",
    "Secret": "${JWT_SECRET_KEY}",
    "SecretKey": "${JWT_SECRET_KEY}",
    "Issuer": "mytrader-api-prod",
    "Audience": "mytrader-client-prod",
    "ExpirationMinutes": 60
  },
  "AuthTestMode": false,
  "FeatureFlags": {
    "EnableAlpacaStreaming": true
  },
  "Alpaca": {
    "Streaming": {
      "Enabled": true,
      "ApiKey": "${ALPACA_API_KEY}",
      "ApiSecret": "${ALPACA_API_SECRET}"
    }
  },
  "Cors": {
    "AllowedOrigins": [
      "https://mytrader.tech",
      "https://www.mytrader.tech"
    ],
    "AllowCredentials": true
  }
}